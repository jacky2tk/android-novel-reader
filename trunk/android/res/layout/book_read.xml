<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/TableLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:stretchColumns="0"
    android:shrinkColumns="0">
    <ScrollView android:id="@+id/scrollView1" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <LinearLayout android:layout_width="match_parent" android:layout_height="match_parent">
            <TextView android:id="@+id/txtBookRead" android:layout_width="fill_parent" android:layout_height="fill_parent" android:text="@string/app_name"/>
        </LinearLayout>
    </ScrollView>
</TableLayout>